    <style name="AppTheme.NoActionBar" parent="AppTheme">
        <item name="android:windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="mySpinnerItemStyle" parent="Widget.AppCompat.DropDownItem.Spinner">
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">italic|bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="BaseHeadlineStyle" parent="Base.TextAppearance.AppCompat.Headline">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="MediumStyle" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="BodyWithNormalTextStyle" parent="Base.TextAppearance.AppCompat.Body1">
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="BodyWithBoldTextStyle" parent="Base.TextAppearance.AppCompat.Body1">
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">normal</item>
        <item name="android:textSize">@dimen/common_16_sp</item>
    </style>

    <style name="SmallestTextStyle" parent="Base.TextAppearance.AppCompat.Caption">
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">normal</item>
        <item name="android:textSize">@dimen/smallest_text_size</item>
    </style>

    <style name="WelcomeTextStyle" parent="Base.TextAppearance.AppCompat.Display1">
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="BtnBaseMoreStyle" parent="TextAppearance.AppCompat.Caption">
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">@dimen/more_btn_widht</item>
        <item name="android:layout_height">@dimen/more_btn_height</item>
        <item name="android:background">@drawable/btn_more_selector</item>
        <!--<isareeNamename="android:paddingTop">@dimen/top_bottom_padding</item>-->
        <!--<isareeNamename="android:paddingBottom">@dimen/top_bottom_padding</item>-->
    </style>

    <style name="BtnMoreStyle" parent="BtnBaseMoreStyle">
        <!--<isareeNamename="android:paddingLeft">@dimen/activity_horizontal_margin</item>-->
        <!--<isareeNamename="android:layout_marginRight">@dimen/negative_margin</item>-->
    </style>

    <style name="Base.SimpleButton" parent="Base.TextAppearance.AppCompat.Button">
        <item name="android:background">@drawable/selector_simple_btn</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SimpleButton" parent="Base.SimpleButton">
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="HomeItemHeaderStyle">
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="ContactItemPadding">
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/view_between_margin</item>
        <item name="android:paddingBottom">@dimen/view_between_margin</item>
    </style>

    <style name="TagButtonStyle" parent="ContactItemPadding">
        <item name="android:background">@drawable/our_work_selector</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SocialButtonStyle" parent="Base.Widget.AppCompat.ImageButton">
        <item name="android:background">@drawable/social_button_selector</item>
        <item name="android:padding">@dimen/top_bottom_padding</item>
        <item name="android:scaleType">center</item>
    </style>


    <style name="marginLeftRightStyle">
        <item name="android:layout_marginRight">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="marginLeftRightStyleTen">
        <item name="android:layout_marginRight">@dimen/common_10_dp</item>
        <item name="android:layout_marginLeft">@dimen/common_10_dp</item>
    </style>

    <style name="paddingCommonFiveStyle" parent="paddingTopBottomFiveStyle">
        <item name="android:paddingLeft">@dimen/common_5_dp</item>
        <item name="android:paddingRight">@dimen/common_5_dp</item>
    </style>

    <style name="paddingTopBottomFiveStyle">
        <item name="android:paddingTop">@dimen/common_5_dp</item>
        <item name="android:paddingBottom">@dimen/common_5_dp</item>
    </style>

    <style name="paddingCommonFifteenStyle">
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingBottom">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="appBtnStyle">
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_app_btn</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/h5</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">@dimen/common_10_dp</item>
    </style>

        <style name="BtnTheme" parent="BaseBtnTheme">
            <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
            <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        </style>

    <style name="CounterOverFlow" parent="TextAppearance.Design.Counter.Overflow">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/green</item>
    </style>


    <style name="BaseBtnTheme" parent="Base.Widget.AppCompat.Button">
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingBottom">@dimen/half_margin</item>
        <item name="android:paddingTop">@dimen/half_margin</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/btn_text_size</item>
    </style>


    <style name="marginLeftRight">
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_marginRight">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="BtnGreenStyle">
        <item name="android:background">@drawable/selector_green_btn</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/h4</item>
        <item name="android:padding">@dimen/common_10_dp</item>
        <item name="android:gravity">center</item>
    </style>

   //filter
    <style name="ActionBar" parent="Base.ThemeOverlay.AppCompat.ActionBar">
        <item name="titleTextStyle">@style/ActionBar.TitleTextStyle</item>
        <item name="homeAsUpIndicator">@drawable/ic_backcustom_arrow</item>
    </style>

    <style name="ActionBar.TitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="LabelStyle">
        <!--<item name="android:textStyle">bold</item>-->
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textSize">@dimen/h5</item>
        <item name="android:background">@color/colorBg</item>
        <item name="android:paddingBottom">@dimen/common_10_dp</item>
        <item name="android:paddingTop">@dimen/common_10_dp</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="filterStyle" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textColor">@color/purpale_green</item>
    </style>

    <style name="common_margin">
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
    </style>

