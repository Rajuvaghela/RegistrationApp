<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <padding android:bottom="@dimen/common_10_dp" android:left="@dimen/common_10_dp" android:right="@dimen/common_10_dp" android:top="@dimen/common_10_dp" />
                    <stroke android:width="@dimen/common_1_dp" android:color="@color/colorDisableText" />
                </shape>
            </item>
            <item android:drawable="@drawable/ic_upload" />
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <padding android:bottom="@dimen/common_10_dp" android:left="@dimen/common_10_dp" android:right="@dimen/common_10_dp" android:top="@dimen/common_10_dp" />
                    <stroke android:width="@dimen/common_1_dp" android:color="@color/green" />
                </shape>
            </item>
            <item android:drawable="@drawable/ic_upload" />
        </layer-list>
    </item>
</selector>